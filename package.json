{
  "name": "simple-web-socket",
  "version": "1.0.0",
  "description": "This project is the web service for elder-360",
  "main": "app.js",
  "scripts": {
    "build": "tsc --build",
    "dev": "ts-node ./src/app.ts",
    "start:dev": "ts-node-dev --watch --respawn --pretty --transpile-only ./src/app.ts",
    "start:prod": "pm2  start ./dist/app.js",
    "stop:prod": "pm2  stop ./dist/app.js",
    "restart:prod": "pm2  restart ./dist/app.js --watch",
    "monit:prod": "pm2  monit ./dist/app.js --watch",
    "test": "jest"
  },
  "author": "Sridhar",
  "license": "ISC",
  "dependencies": {
    "events": "^3.3.0",
    "express": "^4.18.2",
    "http": "^0.0.1-security",
    "mongodb": "^5.1.0",
    "mongoose": "^6.10.0",
    "socket.io": "^4.6.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/jest": "^29.4.0",
    "@types/node": "^18.14.1",
    "@types/socket.io": "^3.0.2",
    "@types/socket.io-client": "^3.0.0",
    "@types/uuid": "^9.0.1",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "reflect-metadata": "^0.1.13",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsyringe": "^4.7.0",
    "typescript": "^4.9.5"
  }
}
